{
  "name": "Chronote",
  "version": "1.0.1",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "description": "Active tab Timers",
  "content_scripts": [{
    "matches": ["https://www.amazon.co.jp/*"],
    "js": [
      "content.js"
    ]
  }],
  "permissions": ["activeTab","declarativeContent", "storage", "bookmarks", "chrome://favicon/", "contextMenus", "tabs"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
        "19": "icon19.png"
    },
    "default_title": "Ext title",
    "default_popup": "popup.html"
  }
}